server_port = 8085;

cluster {
  server port=$server_port {
    jvm-arg "-Xmx2048M";
  }

  #logger finer com.caucho.lucene.bfs.BfsIndexInput;
  #logger off com.caucho.lucene;
  #logger off com.caucho.config.inject;

  #logger finest;
  #logger finer "examples";
  #logger off "com.caucho.amp.proxy";
}
